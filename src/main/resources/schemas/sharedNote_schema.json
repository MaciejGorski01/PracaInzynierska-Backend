{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SharedNote",
  "description": "SharedNote",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a product",
      "type": "string"
    },
    "note_id": {
      "description": "Id of a note",
